{
  "name": "cooker",
  "version": "0.3.0",
  "main": [
    "dist/css/cooker.css",
    "dist/css/cooker.min.css",
    "dist/js/cooker.js",
    "dist/js/cooker.min.js",
    "less/**"
  ],
  "ignore": [
    ".*",
    "*.md",
    "_config.yml"
  ],
  "dependencies": {
    "bootstrap": "~3.1.1",
    "drawer": "~1.0.0",
    "ashiba": "~0.3.0",
    "font-awesome": "~4.0.3"
  },
  "exportsOverride": {
    "bootstrap": {
      "js": [
        "dist/js/*.js",
        "js/*.js"
      ],
      "less": "less/**"
    },
    "drawer": {
      "js": "dist/js/*.js",
      "css": "dist/css/*.css"
    },
    "ashiba": {
      "css": "src/css/*.css"
    },
    "font-awesome": {
      "fonts": "fonts/**",
      "less": "less/**"
    }
  }
}

